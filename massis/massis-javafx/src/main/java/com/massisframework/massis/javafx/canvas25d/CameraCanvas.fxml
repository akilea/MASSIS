<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.PerspectiveCamera?>
<?import javafx.scene.SubScene?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Region?>

<fx:root maxHeight="-Infinity" maxWidth="-Infinity" minHeight="600.0"
	minWidth="800.0" onMouseDragged="#onMouseDragged" onMouseReleased="#onMouseReleased"
	onScroll="#onScroll" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.65"
	onKeyPressed="#onKeyPressed" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<PerspectiveCamera fx:id="camera" />
		<SubScene fx:id="scene3D" AnchorPane.bottomAnchor="0.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0">
			<root>
				<Region />
			</root>
		</SubScene>
	</children>
</fx:root>
